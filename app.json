{
  "expo": {
    "name": "AI Closet",
    "slug": "ai-closet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aicloset.app"
    },
    "android": {
      "package": "com.aicloset.app"
    },
    "web": {
      "bundler": "webpack",
      "output": "static"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to add clothing items to your closet."
        }
      ]
    ]
  }
}


